                    <xs:sequence>
                      <xs:element name="einzelMenge" minOccurs="1" maxOccurs="unbounded">
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Einzelmenge pro Kategorie</p>
                          </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="kategorie" minOccurs="1" maxOccurs="1" type="saatgutKategorie">
                              <xs:annotation>
                                <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                                  <p>Kategorie</p>
                                </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="gewicht" minOccurs="1" maxOccurs="1" type="xs:float">
                              <xs:annotation>
                                <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                                  <p>Gewicht in dt</p>
                                </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Ableitung Saatgut -->
  <xs:complexType name="beschaffenheitsPruefungSaatgut">
    <xs:complexContent>
      <xs:extension base="beschaffenheitsPruefung">
        <xs:sequence>
