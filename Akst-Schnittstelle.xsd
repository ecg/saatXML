                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="no" />
                            <xs:enumeration value="mE" />
                            <xs:enumeration value="oE" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="grundOhneErfolg" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Grund (ohne Erfolg)</p>
                            <ul>
                              <li>wb = wahrscheinlich befallen</li>
                              <li>pS = zwei positive Screenings</li>
                              <li>kl = klonaler Zusammenhang</li>
                              <li>BB = Befall im Betrieb</li>
                            </ul>
                          </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="wb" />
                            <xs:enumeration value="pS" />
                            <xs:enumeration value="kl" />
                            <xs:enumeration value="BB" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="datum" minOccurs="1" maxOccurs="1" type="xs:date">
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Erfassungsdatum der Ergebnisse RS</p>
                          </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>

          <xs:element name="auslagerungsPruefung" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
