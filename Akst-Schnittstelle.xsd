<?xml version="1.0" encoding="UTF-8" ?>

<xs:schema xmlns="http://www.ag-akst.de/saatxml" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.ag-akst.de/saatxml" version="1.0"
  elementFormDefault="qualified" attributeFormDefault="unqualified">

  <xs:annotation>
    <xs:appinfo>
      <p>
        <strong>SaatXML - Bundeseinheitliche Schnittstelle zum Austausch von Saatgutverkehrsdaten für den Anerkennungsprozess.</strong>
      </p>
      <p>Version 1.0 (Stand: 23.01.2009)</p>
    </xs:appinfo>
    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
