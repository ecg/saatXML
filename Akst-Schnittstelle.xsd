              </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="8" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="partie" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
