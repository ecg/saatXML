            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
      <xs:element name="reihenfolgeNummerBSA" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Reihenfolgenummer des BSA</p>
