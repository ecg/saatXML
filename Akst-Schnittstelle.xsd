      <p>Aufbau der Grundstruktur:</p>
      <p>
        <img src="images/transaktionType.jpg" />
      </p>
    </xs:documentation>
  </xs:annotation>

  <!-- TRANSAKTION BEGINN -->
  <xs:element name="transaktion">
    <xs:annotation>
      <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
        <p>Root-Element</p>
        <p>
          <img src="images/transaktion.jpg" />
        </p>
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="header" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
              <p>Header mit Metadaten</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="absender" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Absender des Dokumentes, neunstellige Adressnummer</p>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[0-9]{9}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="empfaenger" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Empfänger des Dokumentes, neunstellige Adressnummer</p>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[0-9]{9}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="sendeZeit" minOccurs="1" maxOccurs="1" type="xs:dateTime">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Versandzeit</p>
                    <p>Format: yyyy-mm-ddThh:mm:ss.zzz[+/-]xx:xx UTC Time Format Beispiel: 2007-02-17T15:40:32.000+01:00</p>
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:any namespace="##other" processContents="strict" minOccurs="0" maxOccurs="unbounded"></xs:any>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="vorgang" minOccurs="1" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
              <p>Vorgang mit atomaren Daten</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice minOccurs="1" maxOccurs="1">
              <xs:element name="adressen" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Liste mit einem oder mehreren "adresse"-Elementen</p>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="adresse" type="adresse" minOccurs="1" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                          <p>Definition des konkreten "adresse"-Elementes</p>
                          <p>
                            <img src="images/adresse.jpg" />
                          </p>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="sortenZusatzDaten" type="sortenZusatzDaten" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>
                      Sortenzusatzdaten bestehen immer aus fruchtArten, fruchtArtOberGruppen, fruchtArtGruppen, sortenGruppen, auflagen,
                      anerkennungsFaehigkeiten. Alle Listen sind bei Angabe von Sortenzusatzdaten zwingend erforderlich.
                    </p>
                    <p>
                      <img src="images/sortenzusatzdaten.jpg" />
                    </p>
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="sorten" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Liste mit einem oder mehreren "sorte"-Elementen</p>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="sorte" type="sorte" minOccurs="1" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                          <p>Definition des konkreten "sorte"-Elementes</p>
                          <p>
                            <img src="images/sorte.jpg" />
                          </p>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="vermehrungsVorhaben" type="vermehrungsVorhaben" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Definition des konkreten "vermehrungsVorhaben"-Elementes</p>
                    <p>
                      <img src="images/vermehrungsvorhaben.jpg" />
                    </p>
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pruefdatenKriterien" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>"pruefdatenKriterien" enthält ein oder mehrere "pruefdatenKriterium"-Elemente</p>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="pruefdatenKriterium" type="pruefdatenKriterium" minOccurs="1" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                          <p>Definition des konkreten "pruefdatenKriterium"-Elementes</p>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="bezugsloseBeschaffenheitsPruefung" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                    <p>Beschaffenheitsprüfung bei z.B. importierten Sorten aus dem Ausland</p>
                    <p>
                      <img src="images/bezugslosebeschaffenheitspruefung.jpg" />
                    </p>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:choice minOccurs="1" maxOccurs="1">
                      <xs:sequence minOccurs="1" maxOccurs="1">
                        <xs:element name="fruchtArt" minOccurs="1" maxOccurs="1">
                          <xs:annotation>
                            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                              <p>Fruchtartschlüssel BSA für Arten die im Artenverzeichnis stehen: dreistellig</p>
                            </xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="3" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element name="sortenNummer" minOccurs="1" maxOccurs="1">
                          <xs:annotation>
                            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                              <p>Sortennummer (BSA): fünfstellig, bestehend aus Ziffern</p>
                            </xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:pattern value="[0-9]{5}" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                      </xs:sequence>
                      <xs:sequence minOccurs="1" maxOccurs="1">
                        <xs:element name="fruchtArtFreitext" minOccurs="1" maxOccurs="1">
                          <xs:annotation>
                            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                              <p>Fruchtartfreitext für Arten die NICHT im Artenverzeichnis stehen</p>
                            </xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="255" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element name="sortenFreitext" minOccurs="1" maxOccurs="1">
                          <xs:annotation>
                            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                              <p>Sortenfreitext für Sorten die NICHT beim BSA gelistet sind.</p>
                            </xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="255" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                      </xs:sequence>
                    </xs:choice>
                    <xs:element name="bezugsText" minOccurs="0" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                          <p>Bezugstext</p>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="1" />
                          <xs:maxLength value="255" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="beschaffenheitsPruefung" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Definition des abstrakten "beschaffenheitsPruefung"-Elementes und Ableitung in Saatgut/Pflanzgut</p>
                          </xs:documentation>
                        </xs:annotation>
                        <xs:choice minOccurs="1" maxOccurs="1">
                          <xs:element name="beschaffenheitsPruefungPflanzgut" type="beschaffenheitsPruefungPflanzgut" minOccurs="1" maxOccurs="1" />
                          <xs:element name="beschaffenheitsPruefungSaatgut" type="beschaffenheitsPruefungSaatgut" minOccurs="1" maxOccurs="1" />
                        </xs:choice>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:any namespace="##other" processContents="strict" minOccurs="0" maxOccurs="unbounded"></xs:any>
            </xs:choice>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- TRANSAKTION ENDE -->

  <!-- ADRESSE BEGINN -->
  <xs:complexType name="adresse">
    <xs:sequence>
      <xs:element name="adressNummer" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Adressnummer: neunstellig, nur Ziffern</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{9}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="anrede" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Anrede</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="20" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="titel" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Akademischer Titel</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="20" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="firma1" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Firmenname, vollständiger Name</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="firma2" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Firmenzusatz 1, respektive Name</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="firma3" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Firmenzusatz 2, respektive Vorname</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element name="strasse" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                <p>Strasse und Hausnummer</p>
              </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="50" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="plz" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                <p>Amtliche Postleitzahl</p>
              </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="20" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element name="postfach" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                <p>Postfach</p>
              </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="20" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="postfachPlz" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                <p>Postleitzahl bei Angabe eines Postfaches</p>
              </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="20" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:choice>
      <xs:element name="ort" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Ort</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ortsteil" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Ortsteil</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="kreis" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Amtlicher Kennzeichen oder KFZ-Zeichen des Kreises: dreistellig</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="3" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="land" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Land: nach "ISO 3166 Alpha-3" Standard</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[A-Za-z]{3}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="telefon" minOccurs="0" maxOccurs="3">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Telefon, max. 3 Elemente</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="30" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="fax" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Faxnummer</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="30" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="email" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>E-Mail-Adresse</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="internet" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Internetadresse der Website</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="bemerkung" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p>Bemerkung/Kommentar</p>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice minOccurs="0" maxOccurs="1">
        <xs:element name="anerkennungsStelle" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
              <p>Spezifika Anerkennungsstelle</p>
