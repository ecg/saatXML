                <xs:element name="cms" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                      <p>CMS-Erkrankung (Clavibacter michiganensis sol., Bakterielle Ringfäule)</p>
                    </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="ergebnis" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Ergebnis CMS</p>
                            <ul>
                              <li>no = noch offen</li>
                              <li>mE = mit Erfolg</li>
                              <li>oE = ohne Erfolg</li>
                            </ul>
                          </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="no" />
                            <xs:enumeration value="mE" />
                            <xs:enumeration value="oE" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="grundOhneErfolg" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Grund (ohne Erfolg)</p>
                            <ul>
                              <li>wb = wahrscheinlich befallen</li>
                              <li>pS = zwei positive Screenings</li>
                              <li>kl = klonaler Zusammenhang</li>
                              <li>BB = Befall im Betrieb</li>
                            </ul>
                          </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="wb" />
                            <xs:enumeration value="pS" />
                            <xs:enumeration value="kl" />
                            <xs:enumeration value="BB" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="datum" minOccurs="1" maxOccurs="1" type="xs:date">
                        <xs:annotation>
                          <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                            <p>Erfassungsdatum der Ergebnisse CMS</p>
                          </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="rs" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                      <p>RS-Erkrankung (Ralstonia solanacearum, Schleimkrankheit)</p>
